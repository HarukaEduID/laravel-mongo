{
 "name": "symfomany/laravel-mongo",
 "description": "A MongoDB based Eloquent model and Query builder for Laravel 5",
 "keywords": ["laravel", "eloquent", "mongodb", "mongo", "database", "model"],
 "homepage": "https://github.com/symfomany/laravel-mongo",
 "version": "1.0",
 "type": "library",
 "authors": [{
  "name": "Boyer Julien",
  "homepage": "https://github.com/symfomany/laravel-mongo"
 }],
 "license": "MIT",
 "require": {
  "illuminate/support": "^5.1",
  "illuminate/container": "^5.1",
  "illuminate/database": "^5.1",
  "illuminate/events": "^5.1",
  "mongodb/mongodb": "^1.0.0@beta"
 },
 "require-dev": {
  "fzaninotto/faker": "~1.4",
  "mockery/mockery": "0.9.*",
  "phpunit/phpunit": "~4.0",
  "phpspec/phpspec": "~2.1",
  "orchestra/testbench": "^3.1",
  "satooshi/php-coveralls": "^0.6"
 },
 "autoload": {
  "psr-4": {
   "Mongo\\": "src/Mongo"
  }
 },
 "autoload-dev": {
  "classmap": [
   "tests/BaseTestCase.php",
   "tests/models",
   "tests/seeds"
  ]
 },
 "minimum-stability": "dev"


}